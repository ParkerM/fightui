    "@babel/core": "^7.2.2",
    "@types/jest": "^23.3.10",
    "@types/jsdom": "^12.2.0",
    "@types/node": "^10.12.18",
    "babel-core": "^7.0.0-beta.3",
    "babel-jest": "^24.0.0-alpha.9",
    "body-parser": "^1.18.3",
    "codelyzer": "^4.5.0",
    "express": "^4.16.4",
    "express-serve-static-core": "^0.1.1",
    "http-proxy-middleware": "^0.19.1",
    "jest": "^24.0.0-alpha.9",
    "jest-preset-angular": "^7.0.0-alpha.2",
    "jest-sonar-reporter": "^2.0.0",
    "jest-spec-reporter": "^1.0.5",
    "rimraf": "^2.6.2",
