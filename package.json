  "license": "Apache-2.0",
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "build:prod": "ng build --aot --prod",
    "build:server": "tsc -p ./server/tsconfig.server.json",
    "build-package:prod": "yarn run build:server && yarn run build:prod && rimraf ./out-tsc",
    "lint": "ng lint",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:vulnerabilities": "snyk test",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.0-rc.0",
    "@angular/cdk": "^7.2.0",
    "@angular/common": "^7.2.0-rc.0",
    "@angular/compiler": "^7.2.0-rc.0",
    "@angular/core": "^7.2.0-rc.0",
    "@angular/forms": "^7.2.0-rc.0",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "^7.2.0-rc.0",
    "@angular/platform-browser-dynamic": "^7.2.0-rc.0",
    "@angular/router": "^7.2.0-rc.0",
    "core-js": "^2.6.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.3.3",
    "tslib": "^1.9.3",
    "zone.js": "^0.8.26"
  },
